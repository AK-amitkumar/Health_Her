<odoo>
    <data>

    	<template id="footer_custom" inherit_id="website.layout" name="Hide Footer">
    		<xpath expr="//div[@id='footer']" position="replace">
    			<div class="oe_structure" id="footer"/>
    		</xpath>
    	</template>

		<template id="layout_footer_copyright_custom" inherit_id="website.layout" name="Hide Footer Copyright">
			<xpath expr="//div/div[@class='pull-left text-muted']" position="replace">
				<div class="pull-left text-muted"/>
			</xpath>
		</template>
		
		<template id="assets_frontend_result" inherit_id="website.assets_frontend" name="Assets">
        	<xpath expr="." position="inside">
	  			<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"/>
	  			<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css"/>
	  			<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    			<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
	  			<script src="https://cdnjs.cloudflare.com/ajax/libs/Mustache.js/2.2.1/mustache.js"></script>
        	</xpath>
        </template>
    
    </data>
</odoo>